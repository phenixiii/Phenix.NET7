	</Reference>
	<Reference Include="Phenix.Services.Contract">
	  <HintPath>..\Bin_MySQL\net5.0\Phenix.Services.Contract.dll</HintPath>
	</Reference>
  </ItemGroup>

  <ProjectExtensions><VisualStudio><UserProperties Reactor_Enabled="1" /></VisualStudio></ProjectExtensions>

