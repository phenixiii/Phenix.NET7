<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<Version>7.6.56</Version>
		<AssemblyVersion>7.6.56.0401</AssemblyVersion>
		<FileVersion>7.6.56.0401</FileVersion>
		<Description>Teamwork Project Tracker 2.0</Description>
		<Authors>ming</Authors>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
		<AssemblyName>Phenix.TPT.Plugin</AssemblyName>
		<RootNamespace>Phenix.TPT.Plugin</RootNamespace>
		<Copyright>Copyright © 2007, 2022 Phenixヾ Studio</Copyright>
		<Company />
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<OutputPath>..\Bin_ORA\</OutputPath>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<OutputPath>..\Bin_ORA\</OutputPath>
	</PropertyGroup>

	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
		<PackageReference Include="Microsoft.Orleans.Client" Version="3.6.2" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
	</ItemGroup>

	<ItemGroup>
		<Reference Include="Phenix.Actor">
			<HintPath>..\Bin_ORA\net6.0\Phenix.Actor.dll</HintPath>
		</Reference>
		<Reference Include="Phenix.Business">
			<HintPath>..\Bin_ORA\net6.0\Phenix.Business.dll</HintPath>
		</Reference>
		<Reference Include="Phenix.Core">
			<HintPath>..\Bin_ORA\net6.0\Phenix.Core.dll</HintPath>
		</Reference>
		<Reference Include="Phenix.Mapper">
			<HintPath>..\Bin_ORA\net6.0\Phenix.Mapper.dll</HintPath>
		</Reference>
	</ItemGroup>

	<ProjectExtensions>
		<VisualStudio>
			<UserProperties Reactor_Enabled="1" />
		</VisualStudio>
	</ProjectExtensions>

</Project>