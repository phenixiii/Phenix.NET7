<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<Version>7.5.52</Version>
		<AssemblyVersion>7.5.52.0920</AssemblyVersion>
		<FileVersion>7.5.52.0920</FileVersion>
		<Description>Phenix Framework 7.5 for .NET 5</Description>
		<Authors>ming</Authors>
		<GeneratePackageOnBuild>false</GeneratePackageOnBuild>
		<AssemblyName>Phenix.Services.Extend</AssemblyName>
		<RootNamespace>Phenix.Services.Extend</RootNamespace>
		<Copyright>Copyright © 2007, 2021 Phenixヾ Studio</Copyright>
		<Company />
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
		<OutputPath>..\Bin_ORA\</OutputPath>
	</PropertyGroup>

	<PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
		<OutputPath>..\Bin_ORA\</OutputPath>
	</PropertyGroup>

	<ItemGroup>
		<FrameworkReference Include="Microsoft.AspNetCore.App" />
		<PackageReference Include="Microsoft.Orleans.Client" Version="3.5.1" />
		<PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
	</ItemGroup>

	<ItemGroup>
		<Reference Include="Phenix.Actor">
			<HintPath>..\Bin_ORA\net5.0\Phenix.Actor.dll</HintPath>
		</Reference>
		<Reference Include="Phenix.Core">
			<HintPath>..\Bin_ORA\net5.0\Phenix.Core.dll</HintPath>
		</Reference>
		<Reference Include="Phenix.Services.Business">
			<HintPath>..\Bin_ORA\net5.0\Phenix.Services.Business.dll</HintPath>
		</Reference>
		<Reference Include="Phenix.Services.Contract">
			<HintPath>..\Bin_ORA\net5.0\Phenix.Services.Contract.dll</HintPath>
		</Reference>
	</ItemGroup>

	<ProjectExtensions>
		<VisualStudio>
			<UserProperties Reactor_Enabled="1" />
		</VisualStudio>
	</ProjectExtensions>

</Project>